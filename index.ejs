    <% if (msg == true) { %>
        <h3>Isso é uma msg de erro!</h3>
    <% } else { %>
        <h3>Nenhum erro!</h3>
    <% } %>

    <% produtos.forEach(function(produto) { %>
        <hr>
        <h3>Cartão de Débito</h3>
        <h4><%= produto.preco %></h4>
    <% }) %>
</body>
</html>
